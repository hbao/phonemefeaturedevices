//  RESOURCE IDENTIFIER

NAME    MIDP

//  INCLUDES
#include <eikon.rh>
#include <avkon.rh>
#include <avkon.mbg>
#include <avkon.rsg>
#include <eikon.rsg>
#include "MIDPView.hrh"

// ---------------------------------------------------------
//   
//    Define the resource file signature 
//    This resource should be empty.
//
// ---------------------------------------------------------
//
RESOURCE RSS_SIGNATURE { }

//
// String resources
//




//----------------------------------------------------
//   
//    r_midpview_hotkeys
//
//----------------------------------------------------
//
RESOURCE HOTKEYS r_midpview_hotkeys
    {
    control=
        {
        HOTKEY 
            { 
            command = EAknCmdExit;
            key = 'e';
            }
        };
    }




//----------------------------------------------------
//   
//    r_midpview_menupane
//
//----------------------------------------------------
//
RESOURCE MENU_PANE r_midpview_menupane
    {
    items=
        {
        };
    }


//----------------------------------------------------
//   
//    r_midpview_menubar
//
//----------------------------------------------------
//
RESOURCE MENU_BAR r_midpview_menubar
    {
    titles =
        {
        MENU_TITLE {menu_pane = r_midpview_menupane;}
        };
    }


//----------------------------------------------------
//   
//    r_midpview_view
//
//----------------------------------------------------
//
RESOURCE AVKON_VIEW r_midpview_view
    {
    menubar = r_midpview_menubar;
    }


